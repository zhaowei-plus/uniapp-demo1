.popup-reward {
  width: 660rpx;
  height: 600rpx;
  padding: 52rpx;
  background-image: url(../../static/img/pup-reward.2acdc279.png);
  background-size: cover;
}
.popup-reward .icon {
    width: 540rpx;
    height: 92rpx;
}
.popup-invite {
  width: 100%;
  padding: 20rpx 20rpx 0;
  background-color: #fff;
}
.popup-invite-header .title {
    height: 50rpx;
    font-size: 14px;
    line-height: 50rpx;
    color: #252A30;
    text-align: center;
}
.popup-invite-content {
    margin-top: 30rpx;
}
.popup-invite-content .channels {
      width: 600rpx;
      display: grid;
      grid-template-columns: repeat(3, 1fr);
      gap: 20rpx;
}
.popup-invite-content .channels .channel .url {
        width: 66rpx;
        height: 68rpx;
}
.popup-invite-content .channels .channel .title {
        height: 40rpx;
        font-size: 14px;
        margin-top: 10rpx;
        line-height: 40rpx;
        color: #252A30;
}
.popup-invite-footer {
    width: 100%;
    height: 80rpx;
}
.popup-invite-footer .btn-cancel {
      font-size: 14px;
      color: gray;
}
.popup-invite-footer .btn-cancel.hover {
        opacity: .8;
}
.popup-rules {
  width: 672rpx;
  border-radius: 8rpx;
  padding-bottom: 30rpx;
  background-color: #fff;
}
.popup-rules-header {
    width: 100%;
    height: 80rpx;
    border-radius: 8rpx 8rpx 0 0;
    font-size: 18px;
    color: #fff;
    line-height: 80rpx;
    background-color: #E25238FF;
}
.popup-rules-content {
    padding: 20rpx;
}
.popup-rules-content ._ul {
      padding: 0;
      margin: 0;
      list-style: none;
}
.popup-rules-content ._ul ._li {
        padding-left: 1em;
        font-size: 14px;
        line-height: 40rpx;
        color: #333;
        text-indent: -.7em;
}
.popup-rules-content ._ul ._li + ._li {
          margin-top: 20rpx;
}
.popup-rules-content ._ul ._li:before {
          margin-right: 10rpx;
          color: #E48B4B;
          content: '\25CF';
}
.popup-rules-footer .i-know {
    width: 300rpx;
    height: 72rpx;
    font-size: 14px;
    line-height: 72rpx;
    color: #fff;
    background-color: #E25238;
}

